{
    "name": "weimob-cloud/php-project-boot",
    "type": "library",
    "license": "MIT",
    "description": "WeimobCloud PHP Application",
    "authors": [
        {
            "name": "weimob-tech",
            "email": "Weimob-tech@weimob.com"
        }
    ],
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "platform": {
            "ext-pcntl": "7.2",
            "ext-posix": "7.2"
        }
    },
    "require": {
        "php": ">=7.2.3",
        "ext-PDO": "*",
        "ext-redis": "*",
        "guzzlehttp/guzzle": "^6.3|^7.0",
        "ext-json": "*",
        "ext-curl": "*",
        "slim/slim": "^3.0",
        "psr/container": "1.0.0",
        "monolog/monolog": "^1.24",
        "netresearch/jsonmapper": ">=4.0.0",
        "symfony/yaml": "^4.3",
        "workerman/workerman": "^3.5",
        "ext-shmop": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0",
        "ext-pcntl": "*",
        "ext-posix": "*"
    },
    "autoload": {
        "psr-4": {
            "WeimobCloudBoot\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "WeimobCloudBootTest\\": "test"
        }
    },
    "scripts": {

    },
    "repositories": {
        "packagist": {
            "type": "composer",
            "url": "https://mirrors.aliyun.com/composer/"
        }
    }
}